<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="280.0" prefWidth="271.0">
         <children>
            <Label fx:id="questionNum" layoutX="25.0" layoutY="14.0" />
            <Label fx:id="questionLabel" layoutX="17.0" layoutY="41.0" prefHeight="17.0" prefWidth="432.0" text="What planet do we live on?" />
            <Button fx:id="nextButton" layoutX="112.0" layoutY="217.0" mnemonicParsing="false" text="Next" />
            <RadioButton fx:id="answer1" layoutX="29.0" layoutY="75.0" mnemonicParsing="false" text="Mars" />
            <RadioButton fx:id="answer2" layoutX="29.0" layoutY="100.0" mnemonicParsing="false" text="The Moon" />
            <RadioButton fx:id="answer3" layoutX="29.0" layoutY="125.0" mnemonicParsing="false" text="The Sun" />
            <RadioButton fx:id="answer4" layoutX="29.0" layoutY="150.0" mnemonicParsing="false" text="Jupiter" />
            <RadioButton fx:id="answer5" layoutX="29.0" layoutY="175.0" mnemonicParsing="false" text="Earth" />
            <Label fx:id="resultLabel" layoutX="119.0" layoutY="249.0" />
         </children>
      </Pane>
   </children>
</GridPane>
